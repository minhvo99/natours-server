{
   "version": 2,
   "builds": [
      {
         "src": "src/index.ts",
         "use": "@vercel/node",
         "config": {
            "distDir": "dist",
            "includeFiles": ["src/**/*.ts"]
         }
      }
   ],
   "rewrites": [
      {
         "source": "/api/v1/(.*)",
         "destination": "/dist/index.js"
      }
   ]
}
