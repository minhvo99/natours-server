{
   "version": 2,
   "buildCommand": "npm run build",
   "outputDirectory": "dist",
   "builds": [
      {
         "src": "dist/index.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "dist/index.js",
         "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
   ],
   "env": {
      "NODE_ENV": "production"
   },
   "functions": {
      "dist/index.js": {
         "maxDuration": 60
      }
   }
}
