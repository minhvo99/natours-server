{
   "version": 2,
   "buildCommand": "npm run build",
   "outputDirectory": "dist",
   "rewrites": [
      {
         "source": "/(.*)",
         "destination": "/dist/index.js"
      }
   ],
   "env": {
      "NODE_ENV": "production"
   },
   "functions": {
      "dist/index.js": {
         "maxDuration": 60
      }
   }
}
